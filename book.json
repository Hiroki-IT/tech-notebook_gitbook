{
    "language": "ja",
    "title": "僕のドキュメントサイト【GitBook版】",
    "description": "This is Hiroki-IT/tech-notebook_gitbook",
    "styles": {
        "website": "styles/website.css"
    },
    "plugins": [
        "intopic-toc",
        "advanced-emoji",
        "back-to-top-button",
        "copy-code-button",
        "prism",
        "-highlight"
    ],
    "pluginsConfig": {
        "intopic-toc": {
            "selector": "",
            "label": "目次",
            "isCollapsed": true,
            "maxDepth": 3,
            "isScrollspyActive": true
        },
        "prism": {
            "css": [
                "prismjs/themes/prism-okaidia.css"
            ]
        }
    }
}
